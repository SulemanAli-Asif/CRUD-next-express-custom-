generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt   DateTime
}

model Item {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  price     Int
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String?
  googleId String? @unique
  name     String
}
